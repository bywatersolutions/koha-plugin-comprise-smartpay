            "x-mojo-to": "Com::ByWaterSolutions::CompriseSmartPAY::API#get_debits",
            "operationId": "GetDebits",
            "tags": [
                "smartpay"
            ],
            "parameters": [
                {
                    "name": "accountlines",
                    "in": "path",
